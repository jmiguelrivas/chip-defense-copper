<resources>
    <!-- Shared attributes -->
    <attr name="icon" format="reference" />
    <attr name="text" format="string" />
    <attr name="url" format="string" />

    <declare-styleable name="IconLink">
        <attr name="icon" />
        <attr name="text" />
        <attr name="url" />
    </declare-styleable>

    <declare-styleable name="IconBadge">
        <attr name="icon" />
        <attr name="text" />
    </declare-styleable>

    <attr name="coinBase" format="color" />
    <attr name="coinOutline" format="color" />
    <attr name="coinShadow" format="color" />
    <attr name="coinMidTone" format="color" />
    <attr name="coinHighlight" format="color" />
    <attr name="coinBrand" format="color" />
    <attr name="coinBrandShadow" format="color" />
    <attr name="coinBrandMidTones" format="color" />
    <attr name="coinBrandHighlight" format="color" />

    <attr name="chipTextColor" format="color" />
</resources>


